{
  "expo": {
    "name": "Summit",
    "slug": "Summit",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": ["android", "ios"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEF156"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to determine nearby school districts"
      },
      "bundleIdentifier": "com.vaporys.Summit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FEF156"
      },
      "package": "com.vaporys.Summit",
      "softwareKeyboardLayoutMode": "pan",
      "blockedPermissions": ["android.permission.ACCESS_FINE_LOCATION"],
      "versionCode": 6
    },
    "androidStatusBar": {
      "barStyle": "dark-content"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png"
        }
      ],
      ["./withAndroidSplashScreen.js"]
    ],
    "userInterfaceStyle": "light",
    "extra": {
      "eas": {
        "projectId": "d59960c3-3bf2-4243-b9d3-22051792b8bc"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-3940256099942544/6300978111",
    "ios_app_id": "ca-app-pub-7084446430900509~8532181858"
  }
}
